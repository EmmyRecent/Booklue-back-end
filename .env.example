# Example .env file for Bookly project
# Copy this file to .env and fill in the required values.

PG_USER="your_postgres_user"           # Database username
PG_HOST="localhost"                     # Database host
PG_DATABASE="bookly"                    # Database name
PG_PASSWORD="your_postgres_password"    # Database password
PG_PORT="5432"                          # Database port

SESSION_SECRET="your_session_secret"    # Secret for session management

# Google OAuth credentials
GOOGLE_CLIENT_ID="your_google_client_id"         # Google Client ID
GOOGLE_CLIENT_SECRET="your_google_client_secret" # Google Client Secret
CALLBACK_URL="http://localhost:5000/auth/google/callback" # OAuth callback URL
FRONTEND_URL="http://localhost:5173"                     # Frontend URL